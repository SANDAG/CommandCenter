<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="nodeServer" class="org.springframework.remoting.rmi.RmiProxyFactoryBean">
        <property name="serviceUrl" value="rmi://#{'${nodeHost}'}:1099/NodeService"></property>
        <property name="serviceInterface" value="com.sandag.commandcenter.node.Server"></property>
    </bean>
</beans>
 